<%= form_for([@course, @course.vios.build]) do |f| %>
	<p>
		<%= f.label :imagename %><br>
		<%= f.text_field :imagename %>
	</p>

	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :text %><br>
		<%= f.text_area :text %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>