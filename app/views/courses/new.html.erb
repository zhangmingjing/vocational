<%= render :partial =>"welcome/navbar"%>
<%= form_for :course, url: courses_path do |f| %>
  <p>
    <%= f.label :cname %><br>
    <%= f.text_field :cname %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', courses_path %>